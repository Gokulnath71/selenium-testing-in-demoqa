from selenium import webdriver
from selenium.webdriver.chrome.service import Service as ChromeService
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.by import By
from selenium.webdriver.common.alert import Alert
from webdriver_manager.chrome import ChromeDriverManager
import time
from selenium.webdriver import ActionChains
from selenium.webdriver.support.select import Select
driver = webdriver.Chrome(service=ChromeService(ChromeDriverManager().install()))

def website():
    driver.get('https://demoqa.com/')
    time.sleep(5)
    driver.close()

def Text():
    driver.get('https://demoqa.com/text-box')
    time.sleep(3)
    full_name=driver.find_element(By.ID,"userName")
    full_name.send_keys("username")
    time.sleep(3)
    email=driver.find_element(By.ID,"userEmail")
    email.send_keys("abc@gamil.com")
    time.sleep(2)
    current_address=driver.find_element(By.ID,"currentAddress")
    current_address.send_keys("welcome to selenium")
    time.sleep(3)
    permanent_address=driver.find_element(By.ID,"permanentAddress")
    permanent_address.send_keys("start to testing")
    time.sleep(3)
    submit=driver.find_element(By.ID,"submit")
    submit.click()
    time.sleep(3)
    details=driver.find_element(By.CLASS_NAME,"border.col-md-12.col-sm-12")
    print(details.text)
    time.sleep(2)
    driver.close()

def checkbox():
    driver.get('https://demoqa.com/checkbox')
    time.sleep(2)
    checkbox_click=driver.find_element(By.CLASS_NAME,"rct-title")
    checkbox_click.click()
    result=driver.find_element(By.ID,"result")
    print(result.text)
    time.sleep(2)
    driver.close()

def radiobtn():
    driver.get('https://demoqa.com/radio-button')
    time.sleep(2)
    radio=driver.find_element(By.CLASS_NAME,"mb-3")
    print(radio.text)
    time.sleep(2)
    radio_button=driver.find_element(By.CLASS_NAME,"custom-control-label")
    radio_button.click()
    time.sleep(2)
    radio_check=driver.find_element(By.CLASS_NAME,"mt-3")
    print(radio_check.text)
    time.sleep(2)
    driver.close()

def webtables():
    driver.get('https://demoqa.com/webtables')
    time.sleep(2)
    add=driver.find_element(By.ID,"addNewRecordButton")
    add.click()
    fn=driver.find_element(By.ID,"firstName")
    fn.send_keys("firstname")
    ln=driver.find_element(By.ID,"lastName")
    ln.send_keys("lastname")
    usermail=driver.find_element(By.ID,"userEmail")
    usermail.send_keys("abc@gmail.com")
    age=driver.find_element(By.ID,"age")
    age.send_keys("20")
    salary=driver.find_element(By.ID,"salary")
    salary.send_keys("150000")
    dpt=driver.find_element(By.ID,"department")
    dpt.send_keys("department")
    submit=driver.find_element(By.ID,"submit")
    submit.click()
    time.sleep(4)
    driver.close()

def button():
    driver.get('https://demoqa.com/buttons')
    time.sleep(2)
    action=ActionChains(driver)
    dc=driver.find_element(By.ID,"doubleClickBtn")
    action.double_click(dc).perform()
    time.sleep(2)
    dc_check=driver.find_element(By.ID,"doubleClickMessage")
    if dc_check.is_displayed():
     print("doubleclick is done")
    else:
     print("doubleclick is not")
    time.sleep(3)
    
    right_click=driver.find_element(By.ID,"rightClickBtn")
    action.context_click(right_click).perform()
    time.sleep(3)
    rc_check=driver.find_element(By.ID,"rightClickMessage")
    if rc_check.is_displayed():
     print("doubleclick is done")
    else:
     print("doubleclick is not")
    time.sleep(3)
    driver.close()

def link():
   driver.get('https://demoqa.com/links')
   time.sleep(3)
   link_check=driver.find_element(By.ID,"simpleLink")
   link_check.click()
   time.sleep(2)
   parent=driver.current_window_handle
   time.sleep(2)
   child=driver.window_handles[1]
   driver.switch_to.window(child)
   time.sleep(2)
   new_check=driver.find_element(By.CLASS_NAME,"banner-image")
   if new_check.is_displayed():
    print("link is found")
   else:
    print("link is not found")
   time.sleep(3)
   driver.switch_to.window(parent)
   print(driver.title)
   time.sleep(2)
   driver.close()

def image():
   driver.get('https://demoqa.com/broken')
   time.sleep(2)
   image=driver.find_element(By.XPATH,"(//img)[3]")
   scr=image.get_attribute('src')
   print(scr)
   time.sleep(2)
   driver.close()

def upload_download():
   driver.get('https://demoqa.com/upload-download')
   time.sleep(2)
   download=driver.find_element(By.ID,"downloadButton")
   download.click()
   time.sleep(3)
   upload=driver.find_element(By.XPATH,"//input[@id='uploadFile']")
   upload.send_keys("c:/Users/Indian/Downloads/sampleFile (2).jpeg")
   time.sleep(2)
   path=driver.find_element(By.ID,"uploadedFilePath")
   print(path.text)
   time.sleep(1)
   driver.close()

def form():
   driver.get('https://demoqa.com/automation-practice-form')
   time.sleep(2)
   state=driver.find_element(By.XPATH,"//div[@id='state']//div[@class=' css-1hwfws3']").click()
   time.sleep(2)
   state_click=driver.find_element(By.ID,"react-select-3-option-1")
   state_click.click()
   time.sleep(3)
   city=driver.find_element(By.XPATH,"//div[@id='city']//div[@class=' css-1hwfws3']").click()
   city_click=driver.find_element(By.ID,"react-select-4-option-1").click()
   time.sleep(2)
   pf_fn=driver.find_element(By.ID,"firstName")
   pf_fn.send_keys("firstname")
   time.sleep(2)
   pf_ln=driver.find_element(By.ID,"lastName")
   pf_ln.send_keys("lastname")
   time.sleep(2)
   pf_mail=driver.find_element(By.ID,"userEmail")
   pf_mail.send_keys("abc@gamil.com")
   time.sleep(2)
   pf_gender=driver.find_element(By.CLASS_NAME,"custom-control-label")
   pf_gender.click()
   time.sleep(2)
   pf_number=driver.find_element(By.ID,"userNumber")
   pf_number.send_keys("9999999999")
   time.sleep(2)
   pf_dob=driver.find_element(By.ID,"dateOfBirthInput").click()
   time.sleep(2)
   dob_month=Select(driver.find_element(By.XPATH,"//select[@class='react-datepicker__month-select']"))
   dob_month.select_by_visible_text('July')
   dob_month.select_by_value('6')
   time.sleep(3)
   dob_year=Select(driver.find_element(By.XPATH,"//select[@class='react-datepicker__year-select']"))
   dob_year.select_by_visible_text('2003')
   dob_year.select_by_value('2003')
   time.sleep(3)
   date=driver.find_element(By.XPATH,"//div[@aria-label='Choose Thursday, July 17th, 2003']").click()
   time.sleep(3)
   hobbies=driver.find_element(By.XPATH,"//label[normalize-space()='Reading']").click()
   time.sleep(3)
   file=driver.find_element(By.XPATH,"//input[@id='uploadPicture']").send_keys("c:/Users/Indian/Downloads/intern logo.webp")
   time.sleep(3)
   address=driver.find_element(By.XPATH,"//textarea[@id='currentAddress']").send_keys("welcome to testing")
   time.sleep(3)
   submit=driver.find_element(By.ID,"submit").click()
   time.sleep(5)
   details=driver.find_element(By.XPATH,"//div[@class='modal-body']")
   print(details.text)
   time.sleep(1)
   driver.close()

def browser_window():
   driver.get("https://demoqa.com/browser-windows")
   time.sleep(2)
   new_window=driver.find_element(By.ID,"windowButton").click()
   time.sleep(2)
   parent=driver.current_window_handle
   child=driver.window_handles[1]
   driver.switch_to.window(child)
   message=driver.find_element(By.ID,"sampleHeading")
   if message.is_displayed():
      print("tab is open")
   else:
      print("no tab is open")
   driver.switch_to.window(parent)
   print(driver.title)
   driver.close()

def alert():
   driver.get('https://demoqa.com/alerts')
   time.sleep(2)
   alert=driver.find_element(By.ID,"alertButton").click()
   time.sleep(3)
   print(driver.switch_to.alert.text)
   driver.switch_to.alert.accept()
   time.sleep(3)
   driver.close()

def confirm_alert():
   driver.get('https://demoqa.com/alerts')
   time.sleep(2)
   alert=driver.find_element(By.ID,"confirmButton").click()
   time.sleep(3)
   print(driver.switch_to.alert.text)
   driver.switch_to.alert.accept()
   time.sleep(2)
   result=driver.find_element(By.ID,"confirmResult")
   print(result.text)
   time.sleep(2)
   driver.close()

def prompt_alert():
   driver.get('https://demoqa.com/alerts')
   time.sleep(2)
   alert=driver.find_element(By.ID,"promtButton").click()
   time.sleep(2)
   print(driver.switch_to.alert.text)
   driver.switch_to.alert.send_keys("gokulnath R")
   time.sleep(2)
   driver.switch_to.alert.accept()
   time.sleep(3)
   result=driver.find_element(By.ID,"promptResult")
   print(result.text)
   driver.close()

def frame():
   driver.get('https://demoqa.com/frames')
   time.sleep(2)
   driver.switch_to.frame("frame1")
   time.sleep(2)
   parent_text=driver.find_element(By.ID,"sampleHeading")
   print(parent_text.text)
   time.sleep(2)
   driver.switch_to.default_content()
   driver.switch_to.frame("frame2")
   child_text=driver.find_element(By.ID,"sampleHeading")
   print(child_text.text)
   time.sleep(2)
   driver.close()

def modal_dialogs():
   driver.get('https://demoqa.com/modal-dialogs')
   time.sleep(2)
   btn=driver.find_element(By.ID,"showSmallModal").click()
   time.sleep(2)
   message=driver.find_element(By.XPATH,"//div[@class='modal-body']")
   print(message.text)
   close_btn=driver.find_element(By.ID,"closeSmallModal").click()
   driver.close()

def slider():
   driver.get('https://demoqa.com/slider')
   time.sleep(2)
   slider=driver.find_element(By.XPATH,"//input[@type='range']")
   action=ActionChains(driver)
   action.move_to_element_with_offset(slider,-20,1).click().perform()
   time.sleep(2)
   driver.close()

def tooltip():
   driver.get('https://demoqa.com/tool-tips')
   time.sleep(2)
   bt=driver.find_element(By.ID,"toolTipButton")
   action=ActionChains(driver)
   action.move_to_element(bt).perform()
   tooltip=driver.find_element(By.XPATH,"//div[@class='tooltip-inner']")
   print(tooltip.text)
   time.sleep(2)
   driver.close()

def sorrtable():
   driver.get('https://demoqa.com/sortable')
   time.sleep(2)
   one=driver.find_element(By.XPATH,"//div[@class='vertical-list-container mt-4']//div[@class='list-group-item list-group-item-action'][normalize-space()='One']")
   four=driver.find_element(By.XPATH,"//div[@class='vertical-list-container mt-4']//div[@class='list-group-item list-group-item-action'][normalize-space()='Four']")
   time.sleep(3)
   action=ActionChains(driver)
   action.click_and_hold(one).move_to_element(four).perform()
   time.sleep(4)
   driver.close()

def resizeable():
   driver.get('https://demoqa.com/resizable')
   time.sleep(2)
   element=driver.find_element(By.XPATH,"//div[@id='resizableBoxWithRestriction']//span[@class='react-resizable-handle react-resizable-handle-se']")
   action=ActionChains(driver)
   action.click_and_hold(element).move_by_offset(110,90).perform()
   time.sleep(3)
   driver.close()

def drag_drop():
   driver.get('https://demoqa.com/droppable')
   time.sleep(2)
   drag=driver.find_element(By.ID,"draggable")
   drop=driver.find_element(By.ID,"droppable")
   action=ActionChains(driver)
   action.click_and_hold(drag).move_to_element(drop).release().perform()
   time.sleep(3)
   check=driver.find_element(By.XPATH,"//p[normalize-space()='Dropped!']")
   print(check.text)
   driver.close()


