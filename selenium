from selenium import webdriver
from selenium.webdriver.chrome.service import Service as ChromeService
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.by import By
from selenium.webdriver.common.alert import Alert
from webdriver_manager.chrome import ChromeDriverManager
import time
from selenium.webdriver import ActionChains
from selenium.webdriver.support.select import Select
driver = webdriver.Chrome(service=ChromeService(ChromeDriverManager().install()))

driver.get('https://demoqa.com/')
time.sleep(5)
elements=driver.find_element(By.CLASS_NAME,"avatar.mx-auto.white")
elements.click()
time.sleep(3)

text=driver.find_element(By.CLASS_NAME,"btn.btn-light ")
text.click()
time.sleep(2)

full_name=driver.find_element(By.ID,"userName")
full_name.send_keys("username")
time.sleep(3)

email=driver.find_element(By.ID,"userEmail")
email.send_keys("abc@gamil.com")
time.sleep(2)

current_address=driver.find_element(By.ID,"currentAddress")
current_address.send_keys("welcome to selenium")
time.sleep(2)

permanent_address=driver.find_element(By.ID,"permanentAddress")
permanent_address.send_keys("start to testing")
time.sleep(2)

submit=driver.find_element(By.ID,"submit")
submit.click()
time.sleep(2)

details=driver.find_element(By.CLASS_NAME,"border.col-md-12.col-sm-12")
print(details.text)
time.sleep(2)

check_box=driver.find_element(By.ID,"item-1")
check_box.click()
time.sleep(2)

checkbox_click=driver.find_element(By.CLASS_NAME,"rct-title")
checkbox_click.click()

result=driver.find_element(By.ID,"result")
print(result.text)
time.sleep(2)

radio_btn=driver.find_element(By.ID,"item-2")
radio_btn.click()
time.sleep(2)

radio=driver.find_element(By.CLASS_NAME,"mb-3")
print(radio.text)
time.sleep(2)
radio_button=driver.find_element(By.CLASS_NAME,"custom-control-label")
radio_button.click()
time.sleep(2)
radio_check=driver.find_element(By.CLASS_NAME,"mt-3")
print(radio_check.text)
time.sleep(2)

web_tables=driver.find_element(By.ID,"item-3")
web_tables.click()
time.sleep(2)

add=driver.find_element(By.ID,"addNewRecordButton")
add.click()

fn=driver.find_element(By.ID,"firstName")
fn.send_keys("firstname")
ln=driver.find_element(By.ID,"lastName")
ln.send_keys("lastname")
usermail=driver.find_element(By.ID,"userEmail")
usermail.send_keys("abc@gmail.com")
age=driver.find_element(By.ID,"age")
age.send_keys("20")
salary=driver.find_element(By.ID,"salary")
salary.send_keys("150000")
dpt=driver.find_element(By.ID,"department")
dpt.send_keys("department")
submit=driver.find_element(By.ID,"submit")
submit.click()
time.sleep(4)

button=driver.find_element(By.ID,"item-4")
button.click()
action=ActionChains(driver)
dc=driver.find_element(By.ID,"doubleClickBtn")
action.double_click(dc).perform()
time.sleep(2)
dc_check=driver.find_element(By.ID,"doubleClickMessage")
if dc_check.is_displayed():
    print("doubleclick is done")
else:
    print("doubleclick is not")
action=ActionChains(driver)
right_click=driver.find_element(By.ID,"rightClickBtn")
action.context_click(right_click).perform()
time.sleep(3)
rc_check=driver.find_element(By.ID,"rightClickMessage")
if rc_check.is_displayed():
    print("doubleclick is done")
else:
    print("doubleclick is not")
    time.sleep(3)
click_btn=driver.find_element(By.ID,"FzVnG")
click_btn.click()
time.sleep(3)
click_check=driver.find_element(By.ID,"dynamicClickMessage")
if click_check.is_displayed():
    print("click is done")
else:
    print("click is not done")
    time.sleep(3)
time.sleep(2)
link=driver.find_element(By.ID,"item-5")
link.click()
link_check=driver.find_element(By.ID,"simpleLink")
if link_check.is_displayed():
    print("link is found")
else:
    print("link is not found")
time.sleep(3)
image=driver.find_element(By.ID,"item-6")
image.click()
driver.find_element("tag name","img").click()
time.sleep(2)
upload_download=driver.find_element(By.ID,"item-7")
upload_download.click()
time.sleep(2)
download=driver.find_element(By.ID,"downloadButton")
download.click()
time.sleep(3)
upload=driver.find_element(By.ID,"uploadFile")
upload.send_keys("sampleFile (1).jpeg")
time.sleep(2)
dynamic=driver.find_element(By.ID,"item-8")
dynamic.click()
dynamic_text=driver.find_element(By.ID,"s4a2d")
print(dynamic_text.text)
time.sleep(2)
form=driver.find_element(By.CLASS_NAME,"header-wrapper")
form.click()
pf=driver.find_element(By.ID,"item-0")
pf.click()
pf_fn=driver.find_element(By.ID,"firstName")
pf_fn.send_keys("firstname")
pf_ln=driver.find_element(By.ID,"lastName")
pf_ln.send_keys("lastname")
pf_mail=driver.find_element(By.ID,"userEmail")
pf_mail.send_keys("abc@gamil.com")
pf_gender=driver.find_element(By.CLASS_NAME,"custom-control-label")
pf_gender.click()
pf_number=driver.find_element(By.ID,"userNumber")
pf_number.send_keys("9999999999")
pf_dob=driver.find_element(By.ID,"dateOfBirthInput")
pf_dob.click()
dob_month=driver.find_element(By.CLASS_NAME,"react-datepicker__month-select")
dob_select=Select(dob_month)
dob_select.select_by_visible_text("Feb")
time.sleep(3)
dob_year=driver.find_element(By.CLASS_NAME,"react-datepicker__year-select")
year_select=Select(dob_year)
year_select.select_by_visible_text("2003")
time.sleep(3)
pf_subject=driver.find_element(By.CLASS_NAME,"subjects-auto-complete__control.css-yk16xz-control")
pf_subject.send_keys("welcome")
pf_check=driver.find_element(By.CLASS_NAME,"custom-control-label")
pf_check.click()
pf_upload=driver.find_element(By.ID,"uploadPicture")
pf_upload.send_keys("sampleFile (1).jpeg")
pf_ca=driver.find_element(By.ID,"currentAddress")
pf_ca.send_keys("new testing")
pf_state=driver.find_element(By.CLASS_NAME," css-tlfecz-indicatorContainer")
state_select=Select(pf_state)
state_select.select_by_visible_text("Rajasthan")
time.sleep(2)
pf_city=driver.find_element(By.CLASS_NAME,"css-19bqh2r")
city_select=Select(pf_city)
city_select.select_by_visible_text("Agra")
time.sleep(2)
driver.close()
